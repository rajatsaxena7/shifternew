                            <div class="ppc-card-data">
                                <h3><%=language.Total_Delivered_Order%></h3>
                                <div class="ppc-card-footer-data">
                                    <button class="btn btn-success mt-3" ><%=delivered[0].tot_delivered%></button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-4 col-md-6 col-sm-6">

                                <div class="ppc-card-footer-data">
                                    <button class="btn btn-info mt-3 invosymbol text-white" ><%=tot_amount%></button>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
            <div class="text-left">
                <div class="ppc-default-content large">
                    <h4 class="font-weight-bold"><%=language.Recent_Transactions%></h4>
                </div>
            </div>

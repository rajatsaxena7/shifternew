<div class="section  ppc-section-padding2">
    <div class="container">
      <div class="ppc-section-title">
        <div class="ppc-default-content large">
          <h2><%=language.Shiftr_users_share_their_experiences%></h2>
          
        </div>
      </div>
    </div>
    <div class="ppc-testimonial-slider">

      <% cus_experience.forEach((data)=>{ %>
        <div class="ppc-testimonial-card">
          <h4><%=data.title%></h4>
          <p><%=data.description%></p>
          <div class="ppc-testimonial-author">
            <div class="ppc-testimonial-author-wrap">
              <div class="ppc-testimonial-author-thumb">
                <img src="../../uploads/<%=data.image%>" alt="">
              </div>
              <div class="ppc-testimonial-author-data">
                <h5><%=data.name%></h5>
                <span><%=data.email%></span>
              </div>
            </div>
          </div>
        </div>
      <% }); %>
    </div>
  </div>

  <div class="bg-gray-500 position-relative section  ppc-section-padding2">
    <div class="container">
      <div class="ppc-section-title">
        <div class="ppc-default-content sm">
          <h2><%=language.From_anywhere_to_everywhere%></h2>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12">
          <div class="ppc-map-thumb text-center">
            <img src="../../frontend/images/all-img/v3/map.png" class="img-fluid" alt="">

<!DOCTYPE html>
<html lang="en">


<meta http-equiv="content-type" content="text/html;charset=UTF-8" />
<head>
  
  <%- include('./partials/head'); -%>
  <%- include('./partials/frontend_head'); -%>
</head>

<body class="light">
  <%- include('./partials/frontend_header'); -%>
  
  <div class="masco-breadcrumb-section">
    <div class="container">
        <div class="breadcrumbs ppc-inner-padding pb-2">

        </div>
    </div>
  </div>

  <div class="scetion ppc-section-padding pt-0 pb-5">
    <div class="container">
      <div class="row d-flex justify-content-center">
        <div class="col-lg-10 ">
          <div class="">
            <div class="text-left">
                <div class="ppc-default-content large pb-3">
                    <h4 class="font-weight-bold"></h4>
                </div>
            </div>
            <div class="card-body pb-0">
              <% faq_list.forEach(function(faq){ %>
                <div class="card m-3 p-3 w-100">
                  <h6><%=faq.faq_title%></h6>
                  <p><%=faq.faq_des%></p>
                </div>
              <% }); %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Footer  -->

  <%- include('./partials/frontend_footer'); -%>
  <%- include('./partials/frontend_script'); -%>
  <%- include('./partials/script'); -%>
</body>

</html>